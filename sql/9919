/*
Find the highest salary among salaries that appears only once.

Table: employee
id:                        int
first_name:                varchar
last_name:                 varchar
age:                       int
sex:                       varchar
employee_title:            varchar
department:                varchar
salary:                    int
target:                    int
bonus:                     int
email:                     varchar
city:                      varchar
address:                   varchar
manager_id:                int

*/

SELECT MAX(DISTINCT salary)
FROM employee;
